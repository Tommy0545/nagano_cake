
  <h4>商品一覧（全<%= @items_count %>件）</h4>

      <% @items.each do |item| %>
        
        <%= link_to public_item_path(item.id) do %>
        <%= image_tag item.image, size: "200x200" %>
        <% end %>
        
        <%= item.name %>
        
        <%= item.price %>円
      <% end %>
  
  
 
  <%= paginate @items_page %>
